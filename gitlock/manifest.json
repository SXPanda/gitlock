{

  "manifest_version": 2,
  "name": "GitLock",
  "version": "0.1",

  "description": "Adds a red border to github pull request pages",

  "icons": {
    "48": "icons/gitlock-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.github.com/*"],
      "js": ["assets/js/gitlock.js"],
      "css": ["assets/css/gitlock.css"]
    }
  ]

}
